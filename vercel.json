{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/telegram",
      "dest": "/api/telegram.js"
    },
    {
      "src": "/api/media",
      "dest": "/api/media.js"
    },
    {
      "src": "/api/news.*",
      "dest": "/api/news.js"
    },
    {
      "src": "/api/businesses.*",
      "dest": "/api/businesses.js"
    },
    {
      "src": "/api/reactions",
      "dest": "/api/reactions.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/news.html"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/public/$1"
    }
  ]
}
