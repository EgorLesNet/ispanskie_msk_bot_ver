{
  "functions": {
    "api/router.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/telegram.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/news.html"
    },
    {
      "source": "/api/auth",
      "destination": "/api/router"
    },
    {
      "source": "/api/auth/:action",
      "destination": "/api/router"
    },
    {
      "source": "/api/businesses",
      "destination": "/api/router"
    },
    {
      "source": "/api/digest",
      "destination": "/api/router"
    },
    {
      "source": "/api/digest/:action",
      "destination": "/api/router"
    },
    {
      "source": "/api/media",
      "destination": "/api/router"
    },
    {
      "source": "/api/media/:fileId",
      "destination": "/api/router"
    },
    {
      "source": "/api/news",
      "destination": "/api/router"
    },
    {
      "source": "/api/photo/:fileId",
      "destination": "/api/router"
    },
    {
      "source": "/api/profile",
      "destination": "/api/router"
    },
    {
      "source": "/api/push",
      "destination": "/api/router"
    },
    {
      "source": "/api/push/:action",
      "destination": "/api/router"
    },
    {
      "source": "/api/reactions",
      "destination": "/api/router"
    },
    {
      "source": "/api/reviews",
      "destination": "/api/router"
    },
    {
      "source": "/api/summary",
      "destination": "/api/router"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
