{
  "version": 2,
  "routes": [
    {
      "src": "/api/telegram",
      "dest": "/api/telegram.js"
    },
    {
      "src": "/api/media",
      "dest": "/api/media.js"
    },
    {
      "src": "/api/news",
      "dest": "/api/news.js"
    },
    {
      "src": "/api/businesses(.*)",
      "dest": "/api/businesses.js"
    },
    {
      "src": "/api/reactions",
      "dest": "/api/reactions.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
