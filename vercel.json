{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "index.js"    },
    {
      "dest": "public/$1"    },
    {
      "src": "/(.+\\.html)",      "dest": "public/$1"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    }
  ]
}
