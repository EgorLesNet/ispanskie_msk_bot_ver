{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(?:js|css|png|jpg|jpeg|gif|ico|svg|woff2?|ttf|eot))",
      "dest": "/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/news.html",
      "dest": "/news.html"
    },
    {
      "src": "/business.html",
      "dest": "/business.html"
    },
    {
      "src": "/recommendations.html",
      "dest": "/recommendations.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
